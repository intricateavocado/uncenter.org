		"markdown-it-mark": "^3.0.1",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"markdown-it-task-checkbox": "^1.0.6",
		"md5": "^2.3.0",
		"nord": "^0.2.1",
		"path": "^0.12.7",
		"postcss": "^8.4.21",
		"rimraf": "^4.4.1",
		"sass": "^1.60.0",
		"shiki": "^0.14.1",
		"tailwind-nord": "^1.3.0",
		"tailwindcss": "^3.2.7",
		"turndown": "^7.1.1"
	},
	"devDependencies": {
		"chalk": "^4.1.2",
		"cspell": "^6.31.1",
		"dotenv": "^16.0.3",
