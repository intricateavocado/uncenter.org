{
    "name": "inte11igent",
    "version": "1.0.0",
    "description": "An intelligent starter for Eleventy using TailwindCSS, SASS/SCSS, and more.",
    "scripts": {
        "clean": "rm -rf dist",
        "start": "NODE_ENV=development eleventy --serve & tailwindcss -i ./src/assets/css/tailwind.css -o ./dist/assets/css/tailwind.css --watch & sass --watch ./src/assets/css:./dist/assets/css",
        "build": "NODE_ENV=production eleventy & tailwindcss -i ./src/assets/css/tailwind.css -o ./dist/assets/css/tailwind.css & sass ./src/assets/css:./dist/assets/css"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/uncenter/inte11igent-starter.git"
    },
    "author": {
        "name": "uncenter",
        "email": "contact@uncenter.org",
        "url": "https://uncenter.org/"
    },
    "homepage": "https://github.com/uncenter/inte11igent-starter",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^2.0.0",
        "@aloskutov/eleventy-plugin-external-links": "^1.4.4",
        "@tailwindcss/typography": "^0.5.9",
        "eleventy-plugin-toc": "^1.1.5",
        "fs": "^0.0.1-security",
        "gitlog": "^4.0.4",
        "html-minifier": "^4.0.0",
        "html-prettify": "^1.0.7",
        "html-to-text": "^9.0.4",
        "luxon": "^3.2.1",
        "markdown-it": "^13.0.1",
        "markdown-it-abbr": "^1.0.4",
        "markdown-it-anchor": "^8.6.7",
        "markdown-it-attrs": "^4.1.6",
        "markdown-it-container": "^3.0.0",
        "markdown-it-emoji": "^2.0.2",
        "markdown-it-footnote": "^3.0.3",
        "markdown-it-kbd": "^2.2.2",
        "markdown-it-link-attributes": "^4.0.1",
        "markdown-it-mark": "^3.0.1",
        "markdown-it-sub": "^1.0.0",
        "markdown-it-sup": "^1.0.0",
        "markdown-it-task-checkbox": "^1.0.6",
        "nord": "^0.2.1",
        "path": "^0.12.7",
        "pretty": "^2.0.0",
        "sass": "^1.58.3",
        "striptags": "^3.2.0",
        "tailwind-nord": "^1.3.0"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.13",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "tailwindcss": "^3.2.7"
    }
}
