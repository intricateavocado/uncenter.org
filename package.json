{
	"name": "uncenter.org",
	"version": "5.0.0",
	"description": "Ducks, badly-written articles, some half-baked ideas, and a little bit of everything else.",
	"scripts": {
		"clean": "rimraf dist",
		"spell:run": "cspell src/posts/**/*.md",
		"spell:update": "node ./utils/cspell.js",
		"spell": "npm run spell:update && npm run spell:run",
		"prettier": "prettier --write src/**/*.{njk,js,scss,json}",
		"site": "node ./utils/site.js",
		"restart": "npm run clean && npm run start",
		"start": "NODE_ENV=development eleventy --serve --incremental",
		"build": "NODE_ENV=production eleventy",
		"prod": "npm run spell && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/uncenter/uncenter.org.git"
	},
	"author": {
		"name": "uncenter",
		"email": "contact@uncenter.org",
		"url": "https://uncenter.org/"
	},
	"homepage": "https://www.uncenter.org",
	"license": "MIT",
	"dependencies": {
		"@11ty/eleventy": "^2.0.0",
		"@11ty/eleventy-fetch": "^3.0.0",
		"@11ty/eleventy-img": "^3.0.0",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@aloskutov/eleventy-plugin-external-links": "^1.4.4",
		"@tailwindcss/typography": "^0.5.9",
		"autoprefixer": "^10.4.14",
		"cross-spawn": "^7.0.3",
		"cssnano": "^5.1.15",
		"eleventy-plugin-icons": "^2.4.3",
		"eleventy-plugin-toc": "^1.1.5",
		"fs": "^0.0.1-security",
		"gitlog": "^4.0.4",
		"html-minifier": "^4.0.0",
		"html-prettify": "^1.0.7",
		"html-to-text": "^9.0.5",
		"image-size": "^1.0.2",
		"linkedom": "^0.14.26",
		"lodash": "^4.17.21",
		"lucide-static": "^0.128.0",
		"luxon": "^3.2.1",
		"markdown-it": "^13.0.1",
		"markdown-it-abbr": "^1.0.4",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-attrs": "^4.1.6",
		"markdown-it-container": "^3.0.0",
		"markdown-it-emoji": "^2.0.2",
